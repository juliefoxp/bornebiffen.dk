  </div>

  <!-- Billede til højre -->
  <div class="flex-1 flex justify-start md:justify-end mt-26">
    <img 
      src="/img_11.webp" 
      alt="Drage" 
      class="w-2/3 md:w-1/2 lg:w-1/3 object-cover rounded-xl"
    />
  </div>
</section>



<section class="flex px-6 md:px-20 lg:px-32 mt-10 mb-10">
